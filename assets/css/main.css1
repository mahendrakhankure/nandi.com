/*========== Toolbar Component ===========*/
.topbar-wrapper {
  width: 100%;
  height: 45px;
  background: #fbb700;
  position: relative;
  color: #eee;
  height: 45px;
}
.topbar-wrapper .topbar {
  display: flex;
  justify-content: space-between;
  padding: 0.5rem;
}
.topbar-wrapper .topbar .topbar-left-wrapper {
  display: grid;
  grid-template-columns: fit-content(32px) 1fr;
  gap: 0rem 0.2rem;
}
.topbar-wrapper .topbar .topbar-left-wrapper img {
  width: 28px;
}
.topbar-wrapper .topbar .topbar-left-wrapper h3 {
  text-transform: uppercase;
  font-size: 13px;
  font-weight: 700;
  margin-top: 0.3rem;
  color: #111;
}
.topbar-wrapper .topbar .topbar-right-warpper {
  display: grid;
  grid-template-columns: 1fr 32px;
  gap: 0rem 0.3rem;
  text-align: right;
}
.topbar-wrapper .topbar .topbar-right-warpper span.amount {
  font-size: 11px;
  font-weight: 700;
  margin-top: 5px;
  text-align: right;
  color: #111;
}
.topbar-wrapper .topbar .topbar-right-warpper span.user-pic {
  text-align: right;
}
.topbar-wrapper .topbar .topbar-right-warpper span.user-pic i {
  font-size: 13px;
  border-radius: 50%;
  padding: 5px 6px;
  color: #111;
  border: 2px solid #111;
}

/*========== Betting Game Name ===========*/
